{
  "name": "y-memory",
  "version": "9.0.0-0",
  "description": "Memory database adapter for Yjs",
  "main": "./y-memory.node.js",
  "browser": "./y-memory.js",
  "module": "./src/y-memory.js",
  "scripts": {
    "test": "npm run lint",
    "dist": "rollup -c rollup.node.js; rollup -c rollup.browser.js",
    "lint": "standard",
    "debug": "concurrently 'rollup -wc rollup.test.js' 'cutest-serve y-memory.test.js -o'",
    "watch": "rollup -wc rollup.browser.js; rollup -wc rollup.browser.js",
    "postversion": "npm run dist",
    "postpublish": "tag-dist-files --overwrite-existing-tag"
  },
  "files": [
    "y-memory.*"
  ],
  "standard": {
    "ignore": [
      "/y-memory.test.js",
      "/y-memory.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y-js/y-memory.git"
  },
  "keywords": [
    "Yjs",
    "OT",
    "Collaboration",
    "Synchronization",
    "ShareJS",
    "Coweb",
    "Concurrency"
  ],
  "author": "Kevin Jahns <kevin.jahns@rwth-aachen.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/y-js/yjs/issues"
  },
  "homepage": "http://y-js.org",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "chance": "^1.0.10",
    "concurrently": "^3.4.0",
    "cutest": "^0.1.9",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-regenerator-runtime": "^6.23.1",
    "rollup-watch": "^3.2.2",
    "standard": "^10.0.2",
    "tag-dist-files": "^0.1.6"
  },
  "peerDependencies": {
    "yjs": ">= 13.0.0-0 < 14.0.0"
  }
}
